generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Card {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  title       String
  url         String
  image       String?
  notes       String?
  description String?
  domain      String?
  status      String   @default("ok")
  metadata    String?
}
